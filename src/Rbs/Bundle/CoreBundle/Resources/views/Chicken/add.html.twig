<div class="modal-header portlet-title box blue">
    <button type="button" class="close" data-dismiss="modal" aria-hidden="true"></button>
    <h4 class="modal-title">Add Quantity Set For Location</h4>
</div>
<form action="{{ path('chicken_set_in_location_save', {id: chickenSet.id}) }}" method="post" class="action-content-form">
    <div class="modal-body">
        <div class="row">
            <div class="col-md-12">
                <div class="form-group">
                    <label class="col-md-3 control-label required">Item</label>
                    <div class="col-md-5">
                        <input class="form-control" value="{{ chickenSet.item.name }}" disabled="disabled"/>
                    </div>
                </div>
            </div>
        </div>
        </br>
        <div class="row">
            <div class="col-md-12">
                <div class="form-group">
                    <label class="col-md-3 control-label required">Quantity</label>
                    <div class="col-md-5">
                        <input class="form-control" type="text" id="quantity" name="quantity"/>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="modal-footer">
        <button type="submit" class="btn blue green"> ADD </button>
    </div>
</form>